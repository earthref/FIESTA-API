{
  "name": "earthref-fiesta-api",
  "version": "1.0.0",
  "description": "EarthRef's FIESTA API",
  "author": "Rupert Minnett <rminnett@earthref.org>",
  "license": "MIT",
  "keywords": [],
  "private": true,
  "scripts": {
    "public": "rimraf dist/public && copyfiles src/public/* dist/public && copyfiles -u 1 src/openapi.yaml dist",
    "libs": "rimraf dist/v1/libs && copyfiles src/v1/libs/*.js dist/v1/libs",
    "configs": "rimraf dist/v1/configs && copyfiles src/v1/configs/* dist/v1/configs",
    "static": "npm run public && npm run libs && npm run configs",
    "dev": "ts-node-dev --respawn --inspect -r dotenv/config -- src/api.ts",
    "build": "npm run static && tsc --pretty",
    "start": "node dist/api.js",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write **/*.{js,ts}",
    "lint": "eslint . --ext ts --ext js",
    "test": "jest"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.9.1",
    "archiver": "^5.0.2",
    "aws-sdk": "^2.762.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "deepdash": "^5.3.0",
    "dotenv": "^8.2.0",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^9.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "openapi-backend": "^3.6.2",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.0",
    "xlsx": "^0.16.7"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@types/elasticsearch": "^5.0.36",
    "@types/node": "^14.11.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "axios": "^0.20.0",
    "canvas": "^2.6.1",
    "copyfiles": "^2.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "jest": "^26.4.2",
    "jest-watch-typeahead": "^0.6.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  }
}
